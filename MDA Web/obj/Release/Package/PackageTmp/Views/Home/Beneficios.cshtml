@{
    ViewBag.Title = "Beneficios";


    Response.ContentType = "text/html; charset=utf-8";
    Response.Expires = -1; // Date in the past
    Response.Cache.SetCacheability(HttpCacheability.NoCache);
    Response.Cache.SetNoStore();
    Response.Cache.SetRevalidation(HttpCacheRevalidation.AllCaches);
    Response.AddHeader("Pragma", "no-cache");
    Response.AddHeader("X-Frame-Options", "SAMEORIGIN");
    Response.AddHeader("X-XSS-Protection", "1; mode=block");
    Response.AddHeader("Content-Security-Policy", "frame-ancestors 'self'; img-src 'self';");
    Response.AddHeader("Permissions-Policy", "camera=(), fullscreen=self, geolocation=*, microphone=(self)");
    Response.AddHeader("Referrer-Policy", "strict-origin");
    Response.Headers.Remove("x-powered-by");


    System.Web.HttpContext.Current.Session["session.cookie_httponly"] = 1;


    System.Web.HttpContext.Current.Session["session.use_only_cookies"] = 1;

    System.Web.HttpContext.Current.Session["session.cookie_secure"] = 1;
    System.Web.HttpContext.Current.Session["session.cookie_samesite"] = "Strict";

    int SSL_Port = 448;

    //if (!Request.IsSecureConnection)
    //{
    //    string redirect = "https://" + Request.ServerVariables["HTTP_HOST"] + ":" + SSL_Port + Request.ServerVariables["URL"];
    //    Response.Redirect(redirect);
    //}

    Session.Abandon();


}

